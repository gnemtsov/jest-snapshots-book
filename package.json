{
    "name": "jest-snapshots-book",
    "version": "1.0.0",
    "description": "Build a collection of html representations of jest snapshots for each test file",
    "main": "build/index.js",
    "scripts": {
        "dev": "watch \"npm run build\" src",
        "build": "babel src -d build",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "babel": {
        "presets": [
            "latest"
        ]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/gnemtsov/jest-snapshots-book.git"
    },
    "keywords": [
        "jest",
        "snapshots",
        "representation"
    ],
    "author": "gnemtsov",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/gnemtsov/jest-snapshots-book/issues"
    },
    "homepage": "https://github.com/gnemtsov/jest-snapshots-book#readme",
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-preset-latest": "^6.24.1",
        "watch": "^1.0.2"
    }
}